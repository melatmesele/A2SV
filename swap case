def swap_case(s):
    ans = ""
    for i in s:
        if i.islower() and i.isalpha():
            i = i.upper()
            
        elif i.isupper() and i.isalpha():
            i = i.lower()
        
        ans+=i
    return  ans

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
